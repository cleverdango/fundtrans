<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.mybank.fundtrans.domain">
	<class name="FundHolding" table="fund_holding">
		<id name="id">
			<generator class="identity" />
		</id>
		<property name="amount" />
		
		<many-to-one name="finAcc" class="FinAcc">
			<column name="accId"/>
		</many-to-one>
		
		<many-to-one name="fund" class="Fund">
			<column name="fundId"/>
		</many-to-one>
	</class>
</hibernate-mapping>