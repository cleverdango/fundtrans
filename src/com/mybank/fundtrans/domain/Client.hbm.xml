<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.mybank.fundtrans.domain.Client" table="client">
		<id name="IDCard">
			<generator class="assigned"/>
		</id>
		<property name="name"/>
		<property name="sex" />
		<property name="phone"/>
		<property name="address"/>
		<property name="email"/>
		<property name="hobby"/>
		<property name="createTime" />
		<!--one-to-one name="finAcc"/-->
	</class>
</hibernate-mapping>